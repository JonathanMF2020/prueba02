pipeline {
    agent any
    environment {
        REGISTRY = 'myregistry/examen-practico-app'
        IMAGE_TAG = "latest"
        KUBECONFIG_CREDENTIALS_ID = 'kubeconfig-id'
    }
    stages {
        stage('Get docker version ') {
            steps {
                script {
                    sh "docker --version"

                    
                }
            }
        }
        
    }
    post {
        always {
            cleanWs()
        }
    }
}